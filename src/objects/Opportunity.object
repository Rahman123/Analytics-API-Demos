        <fullName>BlockDates</fullName>
        <active>false</active>
        <errorConditionFormula>OR(ISPICKVAL( StageName, &quot;Closed Won&quot;), ISPICKVAL(StageName, &quot;Closed Lost&quot;))</errorConditionFormula>
        <errorMessage>You can&apos;t edit an opportunity that&apos;s already closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_reason_required</fullName>
        <active>true</active>
        <description>Close reason required whenever an opportunity is lost</description>
        <errorConditionFormula>ISPICKVAL( StageName , &quot;Closed Lost&quot; ) &amp;&amp;  ISBLANK( Close_Reason__c )</errorConditionFormula>
        <errorDisplayField>Close_Reason__c</errorDisplayField>
        <errorMessage>Close Reason is required when an opportunity is lost.</errorMessage>
    </validationRules>
